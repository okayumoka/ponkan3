{
  "name": "ponkan3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist; rm -rf ./dist_dev",
    "clear": "npm run clean",
    "release": "rm -rf ./dist; webpack --config webpack.config.prod.js",
    "build": "rm -rf ./dist_dev; webpack --config webpack.config.dev.js",
    "start": "rm -rf ./dist_dev; webpack-dev-server --config webpack.config.dev.js",
    "doc": "typedoc --out ./doc ./src/ts",
    "lint": "tslint ./src/**/*.ts ./src/*.ts",
    "lint-fix": "tslint --fix ./src/*.ts ./src/*.ts",
    "build-test": "webpack --config webpack.config.test.js --mode=development",
    "start-test": "webpack-dev-server --config webpack.config.test.js --mode=development",
    "gen-command-ref-md": "webpack --config webpack.config.manual.js; node ./manual/scripts_tmp/gen_command_ref_md.js > manual/docs/command_ref.md",
    "gen-command-ref-md-watch": "watch 'npm run gen-command-ref-md' ./manual/scripts",
    "build-manual": "manual/scripts/build.sh",
    "test": "npm run start-test"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/howler": "=2.1.0",
    "@types/mocha": "^5.2.6",
    "@types/pixi.js": "=4.8.5",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "howler": "=2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "pixi.js": "=4.8.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
