{
  "name": "ponkan3",
  "version": "1.0.0",
  "description": "Ponkan3 is novel-game-engine for web.",
  "main": "dist/ponkan3.js",
  "types": "dist/ponkan3.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/studiomikan/ponkan3"
  },
  "author": "okayu <studiomikan@gmail.com> (https://studiomikan.net/)",
  "license": "MIT",
  "homepage": "https://github.com/studiomikan/ponkan3",
  "bugs": {
    "url": "https://github.com/studiomikan/ponkan3/issues",
    "email": "studiomikan@gmail.com"
  },
  "scripts": {
    "clean": "rimraf dist dist_dev dist_test",
    "build": "rimraf dist & webpack --config webpack.config.prod.js",
    "start": "rimraf dist_dev & webpack-dev-server --config webpack.config.dev.js",
    "dev": "npm run start",
    "doc": "rimraf doc & typedoc --out ./doc ./src/ts",
    "lint": "eslint ./src/**/*.ts",
    "lint-fix": "eslint --fix ./src/**/*.ts",
    "prettier": "prettier --write ./src/**/*.ts",
    "build-test": "rimraf dist_test & webpack --config webpack.config.test.js",
    "start-test": "rimraf dist_test & webpack-dev-server --config webpack.config.test.js",
    "do-test": "mocha-headless-chrome -f dist_test/test.html",
    "do-test-host": "mocha-headless-chrome -f http://localhost:8080/dist_test/test.html",
    "test": "run-s build-test do-test"
  },
  "dependencies": {
    "howler": "=2.1.3",
    "pixi-filters": "=3.0.3",
    "pixi.js": "=5.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@pixi/filter-adjustment": "=3.0.3",
    "@types/howler": "=2.1.2",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.0.0",
    "mocha-headless-chrome": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.3.2",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5",
    "watch": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
